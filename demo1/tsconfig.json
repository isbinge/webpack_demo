{
    "compilerOptions": {
        "outDir": "./dist",
        "baseUrl": ".",
        "noImplicitAny": true,
        "module": "ESNext",
        "target": "es6",
        "jsx": "react",
        "allowJs": true,
        "allowSyntheticDefaultImports": true,
        "moduleResolution": "node",
        "importHelpers": true,
        "inlineSourceMap": true,
        "paths": {
            "@":["./src"]
        }
    },
    "include": ["./src"],

}